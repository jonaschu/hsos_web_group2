{
  "rules": {
    "products": {
      ".read": "true",
      ".write": "false",
    },
    "userprofiles": {
    	"$uid": {
        ".read": "$uid === auth.uid",
      	".write": "$uid === auth.uid",
        ".validate": "newData.exists() && newData.child('postleitzahl').val().length ==5 && newData.child('liefergebiet').val().matches(/^Nord|Sued$/) && newData.child('anrede').val().matches(/^Herr|Frau$/)"
      	}
    },
  }
}
